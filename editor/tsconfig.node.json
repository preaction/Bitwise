{
  "extends": "@vue/tsconfig/tsconfig.node.json",
  "compilerOptions": {
    "lib": [ "esnext" ],
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "nodenext",
    "composite": true,
    "allowJs": true,
    "verbatimModuleSyntax": false
  },
  "include": ["package.json", "vite.config.ts", "electron/*", "electron/**/*"]
}
